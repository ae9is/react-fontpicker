{
  "name": "react-fontpicker",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "test-all": "turbo run test-all",
    "preview": "turbo run preview",
    "kill": "killall turbo && rm -r /tmp/turbod",
    "upgrade-packages": "turbo run upgrade-packages"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
