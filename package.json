{
  "name": "react-fontpicker-ts",
  "version": "0.0.1",
  "private": true,
  "description": "A Google font picker component for React.js",
  "keywords": [
    "fontpicker",
    "font",
    "picker",
    "react",
    "google",
    "font",
    "fonts",
    "selector",
    "component"
  ],
  "homepage": "https://ae9is.github.io/react-fontpicker",
  "bugs": {
    "url": "https://github.com/ae9is/react-fontpicker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ae9is/react-fontpicker.git"
  },
  "author": {
    "name": "ae9is",
    "email": "react.fontpicker@gmail.com"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css",
    "*.png"
  ],
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rm -rf dist .tsbuildinfo && rm -rf docs",
    "build:tsup": "tsup",
    "build:types": "tsc -p tsconfig.types.json",
    "build:vite": "vite build",
    "dev": "run-s dev:*",
    "dev:vite": "vite --port 3000",
    "format": "prettier --write src",
    "lint": "eslint src",
    "build-font-previews": "php ./build-font-previews.php",
    "preview": "vite preview --outDir docs"
  },
  "tsup": {
    "entry": ["index.ts"],
    "clean": true,
    "splitting": false,
    "dts": false,
    "minify": false,
    "sourcemap": true,
    "target": "ES2015",
    "format": [
      "cjs",
      "esm"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
